    </owl:Class>
    


    <!-- http://www.semanticweb.org/FESMO#Risk -->

    <owl:Class rdf:about="http://www.semanticweb.org/FESMO#Risk">
        <rdfs:comment xml:lang="en">The term risk could be defined as a situation of exposure to danger. Concretely, in an
industrial environment it is the possibility of the occurrence of a failure, and represents the
potential failures that may have an impact on products, equipment or production processes.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/FESMO#RiskETCH -->

    <owl:Class rdf:about="http://www.semanticweb.org/FESMO#RiskETCH">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/FESMO#Risk"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/FESMO#RiskLITHO -->

    <owl:Class rdf:about="http://www.semanticweb.org/FESMO#RiskLITHO">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/FESMO#Risk"/>
