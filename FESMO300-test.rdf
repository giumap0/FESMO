    <owl:Class rdf:about="http://www.semanticweb.org/FESMO#DryEtch">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/FESMO#Etching"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/FESMO#DummyWafer -->

    <owl:Class rdf:about="http://www.semanticweb.org/FESMO#DummyWafer">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/FESMO#FrontEndWafer"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/FESMO#Durable -->

    <owl:Class rdf:about="http://www.semanticweb.org/FESMO#Durable">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/FESMO#Material"/>
        <rdfs:comment xml:lang="en">a type of material used to facilitate manufacturing but not normally consumed in the
process that is removable, reusable, and trackable. Examples include containers, reticles,
and pellicles.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/FESMO#ElectricalWaferSorting -->

    <owl:Class rdf:about="http://www.semanticweb.org/FESMO#ElectricalWaferSorting">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/FESMO#NonTransformativeProcess"/>
        <rdfs:comment xml:lang="en">Electrical Wafer Sorting, also known as Wafer Test or E-Test, is a process where integrated circuits on a semiconductor wafer are tested for electrical functionality before they are cut into individual dies. This step ensures that defective chips are identified and discarded before further investment in packaging and assembly. EWS typically involves probing the wafer with a fine needle to make contact with the chip pads and testing them using specific electrical signals.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/FESMO#Equipment -->

    <owl:Class rdf:about="http://www.semanticweb.org/FESMO#Equipment">
        <rdfs:comment xml:lang="en">Mechanical factor in the factory which plays a role in the manufacturing process.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/FESMO#Etching -->

    <owl:Class rdf:about="http://www.semanticweb.org/FESMO#Etching">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/FESMO#TransformativeProcess"/>
    </owl:Class>
    


