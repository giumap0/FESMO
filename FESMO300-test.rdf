    <owl:Class rdf:about="http://www.semanticweb.org/FESMO#FISM">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/FESMO#Pod"/>
        <rdfs:comment xml:lang="en">Front-opening Interface Mechanical Standard similar to SMIF is a mechanical interface standard used for the handling and transfer of wafer carriers in a clean environment to reduce contamination</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/FESMO#FOSB -->

    <owl:Class rdf:about="http://www.semanticweb.org/FESMO#FOSB">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/FESMO#Box"/>
        <rdfs:comment xml:lang="en">a shipping box (that complies with SEMI M31) with a front-opening interface.
Front Opening Shipping Box</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/FESMO#FOUP -->

    <owl:Class rdf:about="http://www.semanticweb.org/FESMO#FOUP">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/FESMO#Pod"/>
        <rdfs:comment xml:lang="en">Front Opening Unified  Pod</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/FESMO#Fab -->

    <owl:Class rdf:about="http://www.semanticweb.org/FESMO#Fab">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/FESMO#Organization"/>
        <rdfs:comment xml:lang="en">Strategically structured facility that coordinates multiple departments, including production, maintenance, and process management, to achieve efficient semiconductor manufacturing. It functions as the core operational center where all processes and resources are aligned to meet production goals and quality standards.</rdfs:comment>
        <rdfs:label>Fab</rdfs:label>
