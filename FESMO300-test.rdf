        <rdfs:comment xml:lang="en">product at the conclusion of the wafer
fabrication process.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/FESMO#ProductRoute -->

    <owl:Class rdf:about="http://www.semanticweb.org/FESMO#ProductRoute">
